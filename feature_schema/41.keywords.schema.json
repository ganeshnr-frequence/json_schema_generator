{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "label": {
            "type": "string"
        },
        "featuredKeywordsLimit": {
            "type": "integer"
        },
        "modalConfig": {
            "type": "object",
            "properties": {
                "advertiserCategoryHeaderText": {
                    "type": "string"
                },
                "advertiseNameHeaderText": {
                    "type": "string"
                },
                "advertiserTooltip": {
                    "type": "string"
                },
                "advertiserWebsiteHeaderText": {
                    "type": "string"
                },
                "ideasStepText": {
                    "type": "string"
                },
                "ideasTooltip": {
                    "type": "string"
                },
                "additionalKeywordsTooltip": {
                    "type": "string"
                }
            },
            "required": [
                "additionalKeywordsTooltip",
                "advertiseNameHeaderText",
                "advertiserCategoryHeaderText",
                "advertiserTooltip",
                "advertiserWebsiteHeaderText",
                "ideasStepText",
                "ideasTooltip"
            ],
            "additionalProperties": false
        },
        "validations": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "minLength": {
                        "type": "integer"
                    }
                },
                "required": [
                    "message",
                    "type"
                ],
                "additionalProperties": false
            }
        }
    },
    "required": [
        "featuredKeywordsLimit",
        "label",
        "validations"
    ],
    "additionalProperties": false
}