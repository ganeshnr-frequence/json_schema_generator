{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "label": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "placeholder": {
            "type": "string"
        },
        "formLayout": {
            "type": "object",
            "properties": {
                "orderName": {
                    "type": "object",
                    "properties": {
                        "element": {
                            "type": "object",
                            "properties": {
                                "label": {
                                    "type": "string"
                                },
                                "container": {
                                    "type": "string"
                                },
                                "control": {
                                    "type": "string"
