            "properties": {
                "config": {
                    "type": "object",
                    "properties": {
                        "offset": {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string"
                                },
                                "count": {
                                    "type": "integer"
                                }
                            },
                            "required": [
                                "count",
                                "type"
                            ],
                            "additionalProperties": false
                        },
                        "durationConfig": {
                            "type": "object",
                            "properties": {
                                "default": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "duration": {
                                            "type": "integer"
                                        }
                                    },
                                    "required": [
                                        "duration",
                                        "type"
                                    ],
                                    "additionalProperties": false
                                },
                                "min": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "duration": {
                                            "type": "integer"
                                        }
                                    },
                                    "required": [
                                        "duration",
                                        "type"
                                    ],
                                    "additionalProperties": false
                                },
                                "max": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "duration": {
                                            "type": "integer"
                                        }
                                    },
                                    "required": [
                                        "duration",
                                        "type"
                                    ],
                                    "additionalProperties": false
                                }
                            },
                            "required": [
                                "default",
                                "max",
                                "min"
                            ],
                            "additionalProperties": false
                        },
                        "start": {
                            "type": "object",
                            "properties": {
                                "label": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "label"
                            ],
                            "additionalProperties": false
                        },
                        "end": {
                            "type": "object",
                            "properties": {
                                "label": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "label"
                            ],
                            "additionalProperties": false
                        }
                    },
                    "additionalProperties": false
                },
                "subType": {
                    "type": "string"
